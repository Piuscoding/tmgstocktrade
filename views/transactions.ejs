                    <div class="row">
                        <div class="col-2"><i class="fad fa-bags-shopping fa-lg icon-color"></i></div>
                        <div class="col">Buy Crypto</div>
                    </div>
                </a>

                <a href="/calendar"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-calendar-week fa-lg icon-color"></i></div>
                        <div class="col">Calender</div>
                    </div>
                </a>

                <div class="sidebar-header">Help</div>

                <a href="/managers"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-user-check fa-lg icon-color"></i></div>
                        <div class="col">Managers</div>
                    </div>
                </a>

