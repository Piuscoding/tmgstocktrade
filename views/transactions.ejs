    <div id="app-wrapper">
        <div id="app-sidebar-overlay"></div>
        <div id="app-sidebar">

            <nav class="navbar navbar-light">
                <div class="container-fluid">
                    <a class="navbar-brand" href="/dashboard">
                        <img src="https://beta.tmgstock.com/public/img/logo/logo.png" alt="" width=""
                            height="40">
                    </a>
                </div>
            </nav>

            <div class="p-3">
                <div class="sidebar-names"><%=user.fullname%></div>
                <div class="sidebar-sub mt-1"><%=user.email%></div>

                <div class="sidebar-sub mt-3">Portfolio</div>
                <div class="sidebar-currency"><%=user.balance%>
                </div>

                <div class="sidebar-sub mt-3">Trade Percentage</div>
                <div class="progress mt-1">
                    <div class="progress-bar-animated progress-bar progress-bar-striped bg-warning" role="progressbar"
                        style="width: 0%;" aria-valuenow="25" aria-valuemin="0"
                        aria-valuemax="100"><%=user.session%></div>
                </div>
            </div>

            <div class="app-sidebar-links">

                <div class="sidebar-header">Main</div>

                <div class="p-3">
                    <div class="row">
                        <div class="col-12 col-lg-6 mb-3">
                            <button onclick="location.href=`/deposit`" class="btn btn-success w-100"
                                type="button">Deposit</button>
                        </div>
                        <div class="col-12 col-lg-6 mb-3">
                            <button onclick="location.href=`/withdrawal`" class="btn btn-primary w-100"
                                type="button">Withdraw</button>
                        </div>
                    </div>
                </div>

                <a href="/dashboard"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-tachometer-fast fa-lg icon-color"></i></div>
                        <div class="col">Overview</div>
                    </div>
                </a>

                <a href="/deposit"
                    class="app-sidebar-link active-link">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-wallet fa-lg icon-color"></i></div>
                        <div class="col">My Wallets</div>
                    </div>
                </a>

                <a href="/transactions/<%=user._id%>"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-file-invoice-dollar fa-lg icon-color"></i></div>
                        <div class="col">Transactions</div>
                    </div>
                </a>

                <div class="sidebar-header">Account</div>

                <a href="/account/<%=user._id%>"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-user fa-lg icon-color"></i></div>
                        <div class="col">Personal Data</div>
                    </div>
                </a>

                <a href="/verify"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-shield-check fa-lg icon-color"></i></div>
                        <div class="col">Verification</div>
                    </div>
                </a>

                <a href="/settings"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-cogs fa-lg icon-color"></i></div>
                        <div class="col">Settings</div>
                    </div>
                </a>

                <a href="/accountupgrade"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-sort-amount-up fa-lg icon-color"></i></div>
                        <div class="col">Upgrade</div>
                    </div>
                </a>

                <div class="sidebar-header">Market</div>

                <a href="/charts"
                    class="app-sidebar-link ">
                    <div class="row">
                        <div class="col-2"><i class="fad fa-analytics fa-lg icon-color"></i></div>
