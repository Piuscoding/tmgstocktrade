                        <div class="d-md-none" id="app-sidebar-toggle"><i class="fas fa-bars fa-2x"></i></div>
                    </div>

                    <div class="d-none d-md-block col-md-7">
                        <div class="">
                            <h4 class="page-title">Account Transactions</h4>
                        </div>
                    </div>

                    <div class="col-11 col-md-5">
                        <div class="d-flex justify-content-end">
                            <div class="">
                                <img class="me-2 nav-img"
                                    src="/IMG_UPLOADS<%=user.image%>" alt="">
                                <!-- <img class="me-2 nav-img" src="" id="dtt"> -->
                                <button onclick="location.href='/logout'"
                                    class="btn btn-sm btn-outline-primary">Log Out</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

                <div class="container-fluid mt-3">
        <div class="mt-3 mb-5">
            <h4 class="content-title"><i class="fad fa-money-check-alt"></i> My Transactions</h4>
            <p class="content-desc">Below are an overview of all transactions on your trading account.</p>
        </div>

        <div class="custom-container p-3">
            <%if(user.widthdraws != ''){%>
            <table class="table table-dark data-table w-100">
                <thead>
                    <tr>
                        <!-- <th>S/N</th> -->
                        <th>Amount</th>
                        <th>Status</th>
                        <th>Date</th>
                    </tr>
                </thead>
                <tbody class="mb-3 mt-3">
                    <% if(user.widthdraws.length > 0 ){ %>
                        <%  user.widthdraws.forEach(widthdraw => {  %>
                        <tr>
                            <th><%=widthdraw.type%></th>
                            <th><%=widthdraw.amount%></th>
                            <th><%=widthdraw.status%></th>
                            <th><%=widthdraw.createdAt%></th>
                        </tr>
                        <%  }) %>
                        <%  } %>
                </tbody>
            </table>
            <% }else{%>
                <h3 class="text-center text-secondary mt-5">No Widthdrawal Transaction yet found!</h3>
                <% } %>

        </div>
    </div>

